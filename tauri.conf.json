{
  "$schema": "https://schema.tauri.app/config/2.0.0",
  "productName": "Entavi",
  "version": "0.1.0-beta.1",
  "identifier": "com.entavi.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devUrl": "http://localhost:1420",
    "frontendDist": "dist"
  },
  "plugins": {},
  "app": {
    "windows": [
      {
        "label": "main",
        "fullscreen": false,
        "resizable": false,
        "title": "Entavi",
        "width": 400,
        "height": 520,
        "center": true,
        "visible": false,
        "decorations": true,
        "skipTaskbar": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico",
      "icons/icon.png"
    ]
  }
}
